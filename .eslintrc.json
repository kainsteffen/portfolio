{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "settings": {
    "import/ignore": [".scss$"]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "global-require": 0,
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-props-no-spreading": 0
  }
}
